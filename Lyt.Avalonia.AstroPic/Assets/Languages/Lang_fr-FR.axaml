    <system:String x:Key="Collection.SetWallpaper">En Fond d'Ecran</system:String>
    <system:String x:Key="Collection.Remove">Supprimer</system:String>
    <system:String x:Key="Collection.SaveToDesktop">Sauv. sur Desktop</system:String>
